{
  "devDependencies": {
    "@angular/cli": "^12.1.4",
    "@angular/compiler-cli": "^12.1.4",
    "webpack": "^5.64.4"
  },
  "scripts": {
    "client": "cd frontend && yarn start",
    "server": "nodemon backend/server.js",
    "build": "cd frontend && npm run build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start:prod": "cd frontend/src/app ng serve && node cd ../../../backend/server.js",
    "heroku-postbuild": "cd frontend && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "name": "thecourierclub",
  "description": "courier services",
  "version": "1.0.0",
  "engines": {
    "node": "14.17.0"
  }
}
